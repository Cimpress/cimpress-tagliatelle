{
  "name": "cimpress-tagging-service",
  "version": "0.0.1",
  "description": "A thin client for Cimpress Tagging service",
  "main": "index.js",
  "scripts": {
    "test": "eslint --ext .js src; jest --coverage; ./node_modules/.bin/jest-coverage-ratchet",
    "build": "node ./node_modules/babel-cli/bin/babel.js src -d lib --copy-files"
  },
  "author": "TrdelnikSquad@cimpress.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cimpress/cimpress-tagging-service.git"
  },
  "bugs": {
    "url": "https://github.com/Cimpress/cimpress-tagging-service/issues"
  },
  "keywords": [
    "cimpress",
    "tagging"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "axios-retry": "^3.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-google": "^0.9.1",
    "jest": "^23.6.0",
    "jest-coverage-ratchet": "^0.2.3",
    "regenerator-runtime": "^0.12.1",
    "xhr-mock": "^2.4.1"
  },
  "jest": {
    "coverageReporters": [
      "json-summary"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 86.84,
        "statements": 86.84,
        "functions": 77.78,
        "branches": 90.48
      }
    }
  }
}
